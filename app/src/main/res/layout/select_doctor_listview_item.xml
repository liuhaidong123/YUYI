    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="188dp"
        android:background="@color/color_white">

        <RelativeLayout
            android:id="@+id/top_rl"
            android:layout_width="match_parent"
            android:layout_height="140dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp">

            <ImageView
                android:id="@+id/doctor_img_head"
                android:layout_width="85dp"
                android:layout_height="110dp"
                android:layout_centerVertical="true"
                android:scaleType="center"
                android:src="@mipmap/main_background4" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="110dp"
                android:layout_centerVertical="true"
                android:layout_toRightOf="@id/doctor_img_head">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="15dp">

                    <TextView
                        android:id="@+id/doctor_name_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:textColor="#6a6a6a"
                        android:textSize="18sp" />

                    <TextView
                        android:id="@+id/doctor_job_tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/doctor_name_tv"
                        android:layout_marginTop="15dp"
                        android:text=""
                        android:textColor="#6a6a6a"
                        android:textSize="15sp" />
                </RelativeLayout>


            </RelativeLayout>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/right_rl"
            android:layout_width="match_parent"
            android:layout_height="33dp"
            android:layout_below="@id/top_rl"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/blue_circle_bg">

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:gravity="center">

                <TextView
                    android:id="@+id/regi_numm"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/regi"
                    android:textColor="@color/color_gua"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/doc_num"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@id/regi_numm"
                    android:text="余号 "
                    android:textColor="@color/color_gua"
                    android:textSize="10sp" />

                <TextView
                    android:id="@+id/num_hao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_toEndOf="@id/doc_num"
                    android:text=""
                    android:textColor="@color/color_gua"
                    android:textSize="10sp" />
            </RelativeLayout>
        </RelativeLayout>

    </RelativeLayout>
</RelativeLayout>