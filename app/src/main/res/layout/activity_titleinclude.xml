<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/color_white"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:id="@+id/ms_top_relayout"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
        <ImageView
            android:clickable="true"
            android:onClick="reBack"
            android:id="@+id/activity_include_imageReturn"
            android:layout_alignParentLeft="true"
            android:paddingStart="15dp"
            android:paddingEnd="15dp"
            android:layout_width="wrap_content"
            android:layout_centerVertical="true"
            android:layout_height="wrap_content"
            android:src="@mipmap/ms_home_return"/>
        <TextView
            android:id="@+id/activity_include_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/ms_home_titleText"
            android:layout_centerInParent="true"
            android:textColor="@color/color_ms_home_titleclolr"
            android:textSize="@dimen/ms_home_Titletextsize17"/>
    </RelativeLayout>
</RelativeLayout>